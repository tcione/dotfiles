{
  "default_hour": 9,
  "linewrap": 79,
  "encrypt": true,
  "default_minute": 0,
  "tagsymbols": "@",
  "editor": "vim",
  "timeformat": "%Y-%m-%d %H:%M",
  "highlight": true,
  "journals": {
    "default": {
      "journal": "~/journal.txt",
      "encrypt": true
    },
    "brief": {
      "journal": "~/journal-brief.txt",
      "encrypt": true
    }
  }
}
